# Library
library(hdf5r)

# Load scRNA-seq data by Lengyel et al
url_len <- "https://datasets.cellxgene.cziscience.com/9e7dd889-c863-46f9-9ab9-99175e23fd75.rds"
output_file_len <- "9e7dd889-c863-46f9-9ab9-99175e23fd75.rds"
download.file(url_len, destfile = output_file_len)
sc_len <- readRDS(output_file_len)

# Load scRNA-seq data by Ulrich et al
url_ul <- "https://datasets.cellxgene.cziscience.com/116cae82-6420-48a8-af2f-bc9f64545788.rds"
output_file_ul <- "116cae82-6420-48a8-af2f-bc9f64545788.rds"
download.file(url_ul, destfile = output_file_ul)
sc_ul <- readRDS(output_file_ul)
